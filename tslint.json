{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
        "no-cosole": false,
        "no-shadowed-variable": false
    },
    "rules": {
        "trailing-comma": [false],
        "no-console": false,
        "no-shadowed-variable": false,
        "variable-name": false,
        "only-arrow-functions": false,
        "no-var-requires": false
    },
    "rulesDirectory": []
}